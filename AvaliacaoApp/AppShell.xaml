<?xml version="1.0" encoding="UTF-8" ?>
<Shell
    x:Class="AvaliacaoApp.AppShell"
    xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
    xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
    xmlns:local="clr-namespace:AvaliacaoApp"
    xmlns:view="clr-namespace:AvaliacaoApp.Views"
    Shell.FlyoutBehavior="Flyout"
    Title="AvaliacaoApp">

    <ShellContent
        Title="Home"
        ContentTemplate="{DataTemplate view:MainPage}"
        Route="MainPage" />
    

    <ShellContent
        Title="Sobre"
        ContentTemplate="{DataTemplate view:SobrePage}"
        Route="SobrePage" />
    

</Shell>
